#!/bin/bash

set -eux

# Copy common utils
mkdir -p ${BOSH_INSTALL_TARGET}/common
cp -a ${BOSH_COMPILE_TARGET}/common/* ${BOSH_INSTALL_TARGET}/common

# Extract cloudwatch_exporter package
cp ${BOSH_COMPILE_TARGET}/cloudwatch_exporter/cloudwatch_exporter-0.4-jar-with-dependencies.jar ${BOSH_INSTALL_TARGET}
